ALTER SESSION SET current_schema = CONSINCO;

-- Quantidade Mensal Notas Mod 6 - Nat 81 - Enegcia El√©trica

SELECT COUNT(SEQNOTA) QTD_DOC, TO_CHAR(DTAENTRADA, 'MM') MES, TO_CHAR(DTAENTRADA, 'YYYY') ANO
FROM CONSINCO.OR_NFDESPESA A WHERE A.CODMODELO IN (06,66) AND A.CODHISTORICO = 81

GROUP BY TO_CHAR(DTAENTRADA, 'MM'), TO_CHAR(DTAENTRADA, 'YYYY')

ORDER BY 3 DESC,2 DESC;

-- Fornecedores Desp Energia Eletrica

SELECT DISTINCT B.NOMERAZAO
FROM CONSINCO.OR_NFDESPESA A LEFT JOIN GE_PESSOA B ON A.SEQPESSOA = B.SEQPESSOA
WHERE A.CODMODELO IN (06,66) AND A.CODHISTORICO = 81

