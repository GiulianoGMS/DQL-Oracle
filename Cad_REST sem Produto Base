ALTER SESSION SET current_schema = CONSINCO;

SELECT A.SEQFAMILIA COD_FAMILIA, A.SEQPRODUTO COD_PRODUTO, 
       A.DESCCOMPLETA DESCRICAO, A.SEQPRODUTOBASE PRODUTO_BASE
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               
FROM   MAP_PRODUTO A
WHERE  DESCCOMPLETA LIKE 'REST/%'
AND SEQPRODUTOBASE IS NULL
